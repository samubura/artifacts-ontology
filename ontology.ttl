  owl:members ( artkl:Artifact
                artkl:Category
                artkl:Feature
                artkl:Function
                artkl:Manual
                artkl:Model
                artkl:Step
                artkl:Workspace
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( artkl:Event
                artkl:ObservableProperty
                artkl:Operation
                artkl:Relationship
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( artkl:InvokeOperationStep
                artkl:ObservePropertyStep
                artkl:WaitForEventStep
              )
] .


